    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.fdb_latexmk"
  ],
  //java配置
  "java.home": "C:\\Language\\jdk-11.0.11",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "C:\\Language\\jdk-11.0.11",
